<div class="banner">
  <div class="container">
    <h2><%= t ".content_page" %></h2>
    <p><%= t ".text_ct" %></p>
  </div>
</div>
<div class="technology">
  <div class="container">
    <div class="col-md-9 technology-left">
      <div class="tech-no">
        <%= render "shared/feed", feed_items: @feed_items %>
      </div>
    </div>
    <div class="col-md-3 technology-right">
      <div class="blo-top1">
        <div class="tech-btm">
          <div class="search-1 wow fadeInDown">
            <form action="#" method="post">
              <input type="search" name="Search" value="Search">
              <input type="submit" value=" ">
            </form>
          </div>
          <h4><%= t ".popular_post" %></h4>
          <%= render "posts/top_post" %>
          <div class="insta wow fadeInDown top-user">
            <h4><%= t ".users" %></h4>
            <ul>
              <%= render @users %>
            </ul>
          </div>
          <p><%= t ".txt" %></p>
        </div>
      </div>
    </div>
    <div class="clearfix"></div>
  </div>
</div>
